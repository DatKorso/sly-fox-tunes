# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# File Management
TEMP_DIR=temp
MAX_FILE_SIZE_MB=2000

# YouTube Cookies (optional, but recommended for avoiding "not a bot" errors)
# Path to Netscape format cookies file exported from your browser
# See docs/COOKIES-SETUP.md for detailed instructions
COOKIES_FILE=youtube_cookies.txt

# Logging
LOG_LEVEL=INFO
